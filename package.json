{
  "name": "okshimel.shop",
  "version": "1.0.0",
  "description": "",
  "main": "backend.js",
  "scripts": {
    "start": "node backend.js",
    "dev": "nodemon backend.js",
    "initialize": "npm i && npm run drop && npm run create && npm run migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "create": "npx sequelize-cli db:create --charset utf8mb4 --collate utf8mb4_general_ci",
    "drop": "npx sequelize-cli db:drop",
    "skeleton": "npx sequelize-cli migration:generate --name NEW_MIGRATION"
  },
  "keywords": [],
  "author": "Andrew Oskolok",
  "license": "ISC",
