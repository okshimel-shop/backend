    "express": "^4.17.1",
    "jimp": "^0.16.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.14",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "remove": "^0.1.5",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "shortid": "^2.2.16",
    "uuid": "^8.3.1"
  }
